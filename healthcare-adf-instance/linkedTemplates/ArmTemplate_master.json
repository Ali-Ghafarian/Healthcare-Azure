{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "healthcare-adf-instance"
		},
		"hos_db_generic_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hos_db_generic_ls'"
		},
		"storagegen2_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storagegen2_ls'"
		},
		"healthcare_adb_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'healthcare_adb_ls'"
		},
		"hos_db_generic_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "healthcare-project-sqlserver.database.windows.net"
		},
		"hos_db_generic_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hos_db_generic_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "alighafarian"
		},
		"storagegen2_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://healthcarestoragegen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "healthcare-adf-instance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"hos_db_generic_ls_password": {
						"value": "[parameters('hos_db_generic_ls_password')]"
					},
					"storagegen2_ls_accountKey": {
						"value": "[parameters('storagegen2_ls_accountKey')]"
					},
					"healthcare_adb_ls_accessToken": {
						"value": "[parameters('healthcare_adb_ls_accessToken')]"
					},
					"hos_db_generic_ls_properties_typeProperties_server": {
						"value": "[parameters('hos_db_generic_ls_properties_typeProperties_server')]"
					},
					"hos_db_generic_ls_properties_typeProperties_database": {
						"value": "[parameters('hos_db_generic_ls_properties_typeProperties_database')]"
					},
					"hos_db_generic_ls_properties_typeProperties_userName": {
						"value": "[parameters('hos_db_generic_ls_properties_typeProperties_userName')]"
					},
					"storagegen2_ls_properties_typeProperties_url": {
						"value": "[parameters('storagegen2_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}